fun main() {
    var timeSpentToday = 300
    var timeSpentYesterday = 400
    println("More screen time than yesterday: ${compare(timeSpentToday, timeSpentYesterday)}")

    timeSpentToday = 300
    timeSpentYesterday = 300
    println("More screen time than yesterday: ${compare(timeSpentToday, timeSpentYesterday)}")

    timeSpentToday = 400
    timeSpentYesterday = 300
    println("More screen time than yesterday: ${compare(timeSpentToday, timeSpentYesterday)}")

}

fun compare(today: Int, yesterday: Int): Boolean {
    return today > yesterday
}
